{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "sourceMap": true,
    "experimentalDecorators": true,
    "outDir": "build/www-tmp/typescript-dist/",
    "rootDir": ".",
    "noImplicitAny": true,
    "jsx": "react",
    "allowJs": true,
    "importHelpers": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "build/www-tmp/scripts/**/*.ts"
  ]
}